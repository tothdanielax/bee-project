{
  "name": "tzsd-skyint-bumblebee-v230316",
  "version": "1.0.0",
  "description": "server for skyint project",
  "main": "server.ts",
  "type": "commonjs",
  "scripts": {
    "start": "nodemon --watch server.ts",
    "test": "jest --coverage",
    "db:fresh": "npm run db:clear && npm run db:migrate && npm run db:seed",
    "db:clear": "npx rimraf database/database.sqlite",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "author": "Tóth Zsolt Dániel",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "sequelize": "^6.32.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
